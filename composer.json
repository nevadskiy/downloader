{
    "name": "nevadskiy/downloader",
    "description": "Download files using PHP and Curl",
    "license": "MIT",
    "keywords": ["downloader", "download", "files", "curl"],
    "authors": [
        {
            "name": "Nevadskiy",
            "email": "nevadskiy@gmail.com"
        }
    ],
    "require": {
        "php": "^7.0|^8.0",
        "ext-curl": "*",
        "psr/log": "^1.1|^2.0|^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0|^8.0",
        "friendsofphp/php-cs-fixer": "^2.19"
    },
    "autoload": {
        "psr-4": {
            "Nevadskiy\\Downloader\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Nevadskiy\\Downloader\\Tests\\": "tests/"
        }
    }
}
